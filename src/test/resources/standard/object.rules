# method access #
(call :obj (:f :args...)) => :obj "." :f "(" :args,,, ")"

# field access #
(get :obj :acc) => :obj "." :acc

# struct no arg #
(struct) => "{}"

# struct one arg #
(struct (:name :content)) => "{" :name ": " :content "}"

# struct multi arg #
(struct (:a :b) (:c :d)...) => "{" :a ": " :b ("," :c ": " :d)... "}"
